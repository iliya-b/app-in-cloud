{
  "EnableRegistration": true,
  "AutoTurnOff": 0,
  "AppInCloud": {
    "DefaultAdminEmail": "Admin0@appincloudtest.net",
    "DefaultAdminPassword": "",
    "RPC": {
      "Host": "",
      "User": "",
      "KeyPath": "",
      "Password": ""
    }
  },
  "Kestrel": {
    "Limits": {}
  },
  "ConnectionStrings": {
    "DefaultConnection": "",
    "HangfireConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "AppInCloud": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "Emulator": {
    "BasePath": "/home/ubuntu/android-cuttlefish/cf",
    "LegacyBasePath": "/home/ubuntu/cuttlefish12/cf",
    "AdbPath": "./bin/adb"
  },
  "ReverseProxy": {
    "Routes": {
      "devices": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/devices/{*any}"
        }
      },
      "infra_config": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/infra_config"
        }
      },
      "polled_connections": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/polled_connections/{*any}"
        }
      }
    },
    "Clusters": {
      "cuttlefish": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8532"
          }
        }
      }
    }
  }
}