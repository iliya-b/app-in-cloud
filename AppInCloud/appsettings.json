{
  "EnableRegistration": true,
  "AutoTurnOff": 0,
  "AppInCloud": {
    "DefaultAdminEmail": "Admin0@appincloudtest.net",
    "DefaultAdminPassword": "",
    "RPC": {
      "Host": "",
      "User": "",
      "KeyPath": "",
      "Password": ""
    }
  },
  "Kestrel": {
    "Limits": {}
  },
  "ConnectionStrings": {
    "DefaultConnection": "",
    "HangfireConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "AppInCloud": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "Emulator": {
    "BasePath": "/home/ubuntu/android-cuttlefish/cf",
    "LegacyBasePath": "/home/ubuntu/cuttlefish12/cf",
    "AdbPath": "./bin/adb"
  },
  "ReverseProxy": {
    "Routes": {
      "devices": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/devices/{*any}"
        }
      },
      "list_devices_cvd1": {
        "ClusterId": "cuttlefish_cvd1",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/list_devices/cvd-1"
        },
        "Transforms": [
          {
            "PathSet": "/list_devices"
          }
        ]
      },
      "connect_client_cvd1": {
        "ClusterId": "cuttlefish_cvd1",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/connect_client/cvd-1"
        },
        "Transforms": [
          {
            "PathSet": "/connect_client"
          }
        ]
      },
      "devices_cvd1": {
        "ClusterId": "cuttlefish_cvd1",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/devices/cvd-1/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/devices/cvd-1"
          }
        ]
      },
      "list_devices_cvd2": {
        "ClusterId": "cuttlefish_cvd2",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/list_devices/cvd-2"
        },
        "Transforms": [
          {
            "PathSet": "/list_devices"
          }
        ]
      },
      "connect_client_cvd2": {
        "ClusterId": "cuttlefish_cvd2",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/connect_client/cvd-2"
        },
        "Transforms": [
          {
            "PathSet": "/connect_client"
          }
        ]
      },
      "devices_cvd2": {
        "ClusterId": "cuttlefish_cvd2",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/devices/cvd-2/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/devices/cvd-2"
          }
        ]
      },
      "infra_config": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/infra_config"
        }
      },
      "polled_connections": {
        "ClusterId": "cuttlefish",
        "AuthorizationPolicy": "authorized",
        "Match": {
          "Path": "/polled_connections/{*any}"
        }
      }
    },
    "Clusters": {
      "cuttlefish_v13": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8532"
          }
        }
      },
      "cuttlefish_cvd1": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8443"
          }
        },
        "HttpRequest": {
          "Version": "1.1",
          "VersionPolicy": "RequestVersionExact"
        }
      },
      "cuttlefish_cvd2": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:8444"
          }
        },
        "HttpRequest": {
          "Version": "1.1",
          "VersionPolicy": "RequestVersionExact"
        }
      }
    }
  }
}